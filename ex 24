#include <stdio.h>
// Insertion Sort
void insertion(int a[],int n){
    int i,j,key;
    for(i=1;i<n;i++){
        key=a[i]; j=i-1;
        while(j>=0 && a[j]>key){ a[j+1]=a[j]; j--; }
        a[j+1]=key;
    }
}
int main(){
    int a[5]={5,2,9,1,3},i;
    insertion(a,5);
    printf("Sorted: ");
    for(i=0;i<5;i++) printf("%d ",a[i]);
    return 0;
}
