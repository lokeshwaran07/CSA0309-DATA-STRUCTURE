#include <stdio.h>
int main() {
    int reg[5]={101,102,103,104,105}, search, i, found=0;
    printf("Enter reg no: ");
    scanf("%d",&search);
    for(i=0;i<5;i++)
        if(reg[i]==search){ found=1; break; }
    if(found) printf("Reg no %d Found",search);
    else printf("Not Found");
    return 0;
}
