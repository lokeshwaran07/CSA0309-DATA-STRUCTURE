#include <stdio.h>
int main(){
    int a[7]={1,2,2,3,1,2,3}, i,j,count;
    int visited[7]={0};
    for(i=0;i<7;i++){
        if(visited[i]) continue;
        count=1;
        for(j=i+1;j<7;j++)
            if(a[i]==a[j]){ count++; visited[j]=1; }
        printf("%d occurs %d times\n",a[i],count);
    }
    return 0;
}
