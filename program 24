#include <stdio.h>
#define INF 9999
#define MAX 10

int n, cost[MAX][MAX];

void prim() {
    int selected[MAX] = {0}, edges = 0, total = 0;
    selected[0] = 1;

    printf("Edges in MST:\n");
    while (edges < n - 1) {
        int min = INF, x = 0, y = 0;
        for (int i = 0; i < n; i++) {
            if (selected[i]) {
                for (int j = 0; j < n; j++) {
                    if (!selected[j] && cost[i][j] < min) {
                        min = cost[i][j]; x = i; y = j;
                    }
                }
            }
        }
        printf("%d - %d (%d)\n", x, y, cost[x][y]);
        total += cost[x][y];
        selected[y] = 1;
        edges++;
    }
    printf("Total cost: %d\n", total);
}

int main() {
    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter cost matrix (9999 for no edge):\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &cost[i][j]);

    prim();
    return 0;
}
